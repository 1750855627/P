[
	parsers.DSL = '"Parser at ..\Tools\PCompiler\DSLParser.dll"',
	modules.PData = '"PData at ..\Libraries\Formula\PData.4ml"',
	parse_ActiveParser = "DSL"
]

model CallUnhandledEvent of PData
{
    eEvt is EventDecl("Evt", AssertMaxInstances(1), NIL).
    eActEvt is EventDecl("ActEvt", AssertMaxInstances(1), NIL).

    mMach is MachineDecl("Mach", FALSE, NIL).


        aAct_Mach is ActionDecl("Act", mMach, `{
            raise(Events.Evt);
        }`).

        smSubm_Mach is StateSetDecl("Subm", mMach).

            sS1_Subm_Mach is StateDecl("S1_Subm_Mach", mMach, `{
                send(this, Events.ActEvt);
                call(States.S2_Subm_Mach);
            }`, NIL).

                TransDecl(sS1_Subm_Mach, eEvt, sS3_Subm_Mach, FALSE).

                Install(sS1_Subm_Mach, eActEvt, aAct_Mach).

            InStateSet(smSubm_Mach, sS1_Subm_Mach).

            sS2_Subm_Mach is StateDecl("S2_Subm_Mach", mMach, `{
                
            }`, NIL).

            InStateSet(smSubm_Mach, sS2_Subm_Mach).

            sS3_Subm_Mach is StateDecl("S3_Subm_Mach", mMach, `{
                assert(false);
            }`, NIL).

            InStateSet(smSubm_Mach, sS3_Subm_Mach).

        MachStart(mMach, sS1_Subm_Mach).

    MainDecl(New(MachType("Mach"), NIL)).
}
